<div class="error">
<% if venue.errors.any? %>
    <% venue.errors.full_messages.each do |message|%>
        <h4><%= message %> </h4>
    <%end%>
<%end%>
</div>

<div class ="edit_form">
    <%= form_for venue do |f|%>
        <p><%= f.label :"Name:" %>
        <%= f.text_field :name %></p>
        <p><%= f.label :"location:" %>
        <%= f.text_field :location %></p>
        <p><%= f.label :"capacity:" %>
        <%= f.number_field :capacity %></p>
        <p><%= f.label :"cost:" %>
        <%= f.number_field :cost %></p>
        <p><%= f.label :"Description:" %>
        <%= f.text_area :description %></p>
        <p><%= f.label :"Approved Caterers:"%>
        <%= f.collection_check_boxes :caterer_ids, Caterer.all, :id, :name %></p>
        <%= f.submit "Save" %>
    <%end%><br>
</div>